{
  "name": "maps-bookmarks",
  "version": "1.0.0",
  "description": "Check bookmarks for chronological order",
  "repository": {
    "type": "git",
    "url": "https://github.com/adequatica/maps-bookmarks"
  },
  "author": "Andrey Enin",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@prettier/plugin-xml": "3.4.1",
    "date-fns": "4.1.0",
    "prettier": "3.4.2",
    "vitest": "3.0.5",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "husky": "9.1.7"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "plugins": [
      "@prettier/plugin-xml"
    ],
    "overrides": [
      {
        "files": [
          "*.kml",
          "*.xml"
        ],
        "options": {
          "printWidth": 1000,
          "singleAttributePerLine": true,
          "xmlSelfClosingSpace": false
        }
      }
    ]
  },
  "scripts": {
    "format": "prettier --write .",
    "test": "vitest run",
    "prepare": "husky"
  }
}
