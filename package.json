{
  "name": "maps-bookmarks",
  "version": "1.0.0",
  "description": "Check bookmarks in chronological order",
  "repository": {
    "type": "git",
    "url": "https://github.com/adequatica/maps-bookmarks"
  },
  "author": "Andrey Enin",
  "license": "MIT",
  "main": "chrono.js",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "date-fns": "2.30.0",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "3.4.1",
    "husky": "8.0.3",
    "prettier": "3.2.5"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "plugins": [
      "@prettier/plugin-xml"
    ],
    "overrides": [
      {
        "files": [
          "*.kml",
          "*.xml"
        ],
        "options": {
          "printWidth": 1000,
          "singleAttributePerLine": true,
          "xmlSelfClosingSpace": false
        }
      }
    ]
  },
  "scripts": {
    "chrono": "node chrono.js",
    "format": "prettier --write ."
  }
}
